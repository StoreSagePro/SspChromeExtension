{
  "manifest_version": 3,
  "name": "StoreSagePro",
  "version": "1.3",
  "description": "Best Amazon Listing Calculator for Sellers",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
